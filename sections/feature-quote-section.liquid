<div id="shopify-section-template--17653238890663__feature_quote_section_8JwgAQ" class="shopify-section feature-quote-section-wrapper">
    
    
   

<div class="feature-quote-section">
  <div class="page-width">
    <div class="feature-quote-section__features">
      
         {%- if section.settings.main_image != blank -%}
        <div class="feature-quote-section__features-image">
          <img 
            src="{{ section.settings.main_image | image_url: width: 1200 }}" 
            alt="{{ section.settings.heading }}"
            loading="lazy"
            sizes="(min-width: 992px) 45vw, 100vw"
          >
        </div>
        {%- endif -%}
      

      <div class="feature-quote-section__features-content">
           
          {%- if section.settings.heading != blank -%}
            <h2 class="feature-quote-section__heading">{{ section.settings.heading }}</h2>
          {%- endif -%}
        

        
          <ul class="feature-quote-section__features-list">
            
               {%- for block in section.blocks -%}
              {%- if block.type == 'feature_item' -%}
                <li class="feature-quote-section__feature-item">
                  
                     <div class="feature-quote-section__feature-icon">
<img src="https://cdn.shopify.com/s/files/1/0930/5807/2907/files/svgviewer-output.svg?v=1765949173" alt="Feature icon" srcset="https://cdn.shopify.com/s/files/1/0930/5807/2907/files/svgviewer-output.svg?v=1765949173" width="50" height="48" loading="lazy">                   </div>
                  

                  <div class="feature-quote-section__feature-item-content">
                    <div class="feature-quote-section__feature-description"> {{ block.settings.text }}</div>
                  </div>
                </li>
            
                {%- endif -%}
            {%- endfor -%}
            
              
            
          </ul>
        
      </div>
    </div>

    
      <div class="feature-quote-section__quote-container">
        
        <div class="feature-quote-section__quote-wrapper">
          <div class="feature-quote-section__quote-marks">
            
              <div class="feature-quote-section__quote-mark feature-quote-section__quote-mark--left">
 {%- if section.settings.quote_left_img != blank -%}
              <img 
                src="{{ section.settings.quote_left_img | image_url: width: 100 }}" 
                alt="Opening quote"
                loading="lazy"
              >
              {%- endif -%}              </div>
            

            <div class="feature-quote-section__quote-text">
              {{ section.settings.quote_text }}
            </div>

            
              <div class="feature-quote-section__quote-mark feature-quote-section__quote-mark--right">
  {%- if section.settings.quote_right_img != blank -%}
              <img 
                src="{{ section.settings.quote_right_img | image_url: width: 100 }}" 
                alt="Closing quote"
                loading="lazy"
              >
              {%- endif -%}              </div>
            
          </div>

          
            <div class="feature-quote-section__quote-author">
              
                <div class="feature-quote-section__quote-author-name">
                  — {{ section.settings.author_name }}

                </div>
              

              
                <div class="feature-quote-section__quote-author-position">
                  {{ section.settings.author_position }}
                </div>
              
            </div>
          
        </div>
      </div>
    
</div>
</div>

 </div>


 
{% schema %}
{
  "name": "Feature Quote Section",
  "settings": [
    {
      "type": "image_picker",
      "id": "main_image",
      "label": "Main Image"
    },
    {
      "type": "text",
      "id": "heading",
      "label": "Heading",
      "default": "BEYOND COST SAVINGS"
    },
    {
      "type": "richtext",
      "id": "quote_text",
      "label": "Quote Text"
    },
    {
      "type": "text",
      "id": "author_name",
      "label": "Author Name",
      "default": "— Tim B., Verified Customer"
    },
    {
      "type": "text",
      "id": "author_position",
      "label": "Author Position",
      "default": "(Former supplement enthusiast)"
    },
    {
      "type": "image_picker",
      "id": "quote_left_img",
      "label": "Left Quote Icon"
    },
    {
      "type": "image_picker",
      "id": "quote_right_img",
      "label": "Right Quote Icon"
    }
  ],
  "blocks": [
    {
      "type": "feature_item",
      "name": "Feature Item",
      "settings": [
       
        {
          "type": "richtext",
          "id": "text",
          "label": "Feature Text"
        }
      ]
    }
  ],
  "max_blocks": 20,
  "presets": [
    {
      "name": "Feature Quote Section"
    }
  ]
}
{% endschema %}

