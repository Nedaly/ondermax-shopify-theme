  </div><script>
document.addEventListener('DOMContentLoaded', function() {
  console.log('ðŸŽ¬ Initializing About Partner video play button sync');
  
  // Find all video containers
  const videoContainers = [
    { id: 'video_about_partner_back_play', muteId: 'video_about_partner_back' },
    { id: 'video_about_partner_overlay_play', muteId: 'video_about_partner_overlay' }
  ];
  
  videoContainers.forEach(container => {
    const videoElement = document.getElementById(container.id);
    if (videoElement) {
      const video = videoElement.querySelector('video');
      const playButton = videoElement.querySelector('.play_icon_new');
      
      if (video && playButton) {
        console.log(`ðŸŽ¬ Setting up sync for ${container.id}`);
        
        // Function to update play button visibility
        function updatePlayButtonState() {
          if (video.paused) {
            playButton.style.display = 'flex';
            console.log(`â–¶ï¸ Video ${container.id} paused - showing play button`);
          } else {
            playButton.style.display = 'none';
            console.log(`â¸ï¸ Video ${container.id} playing - hiding play button`);
          }
        }
        
        // Listen for video state changes
        video.addEventListener('play', updatePlayButtonState);
        video.addEventListener('pause', updatePlayButtonState);
        video.addEventListener('ended', updatePlayButtonState);
        video.addEventListener('loadeddata', updatePlayButtonState);
        video.addEventListener('loadedmetadata', updatePlayButtonState);
        
        // Check autoplay state after a short delay
        setTimeout(() => {
          updatePlayButtonState();
          console.log(`ðŸŽ¬ Initial state check for ${container.id}: paused = ${video.paused}`);
        }, 100);
        
        // Also check periodically in case of autoplay delays
        let checkCount = 0;
        const autoplayCheck = setInterval(() => {
          checkCount++;
          updatePlayButtonState();
          
          // Stop checking after 3 seconds
          if (checkCount >= 6) {
            clearInterval(autoplayCheck);
          }
        }, 500);
        
        // Manual play button click handler (works with existing custom.js)
        playButton.addEventListener('click', function() {
          if (video.paused) {
            video.play().then(() => {
              console.log(`ðŸŽ¬ Manual play triggered for ${container.id}`);
              updatePlayButtonState();
            }).catch(error => {
              console.log(`ðŸŽ¬ Play failed for ${container.id}:`, error);
            });
          }
        });
        
        // Also listen for when videos autoplay and become available
        if (video.readyState >= 2) {
          // Video is already loaded
          updatePlayButtonState();
        }
      }
    }
  });
});
</script></div>

